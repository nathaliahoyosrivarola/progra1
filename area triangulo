// area del triangulo.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main()
{
	float altura, base, area;
	cout<<"ingrese la altura";
	cin>> altura;
	cout<<"ingrese la base";
	cin>> base;
	area= (base*altura)/2;
	cout<< "el area es:" <<area;
	getch();
}
